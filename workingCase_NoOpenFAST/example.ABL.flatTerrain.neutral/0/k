FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    lower
    {
        type            zeroGradient;
    }
    upper
    {
        type            zeroGradient;
    }
    west
    {
        type            cyclic;
    }
    east
    {
        type            cyclic;
    }
    north
    {
        type            cyclic;
    }
    south
    {
        type            cyclic;
    }
}


