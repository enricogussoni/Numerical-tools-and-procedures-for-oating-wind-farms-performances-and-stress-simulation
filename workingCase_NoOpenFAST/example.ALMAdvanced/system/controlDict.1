FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

  //application       ABLSolver;
  application	    windPlantSolver.ALMAdvancedOpenFAST;

  libs              ("libuserfiniteVolume.so" "libuserincompressibleLESModels.so");

  startFrom         startTime;

  startTime         200.0;

  stopAt            endTime;

  endTime           20000.0;

  deltaT            0.5;

  writeControl      adjustableRunTime;

  writeInterval     10.0; 

  purgeWrite        0;

  writeFormat       ascii; //binary;

  writePrecision    12;

  writeCompression  off; //compressed;

  timeFormat        general;

  timePrecision     12;

  runTimeModifiable yes;

  adjustTimeStep    no; //yes;

  maxCo             0.75;

  maxDeltaT         25.0;

  functions
  {
       #include "sampling/sliceDataInstantaneous"
       #include "sampling/boundaryDataPre"
       #include "sampling/probeData1"
  }

